{
  "name": "anac-environment-orchestrator",
  "version": "0.0.1",
  "private": true,
  "main": "anac-environment-orchestrator.js",
  "scripts": {
    "start": "node anac-environment-orchestrator.js -level 2 > eo001.log &",
    "pretest": "eslint --ignore-path .gitignore .",
    "start-docker": "node anac-environment-orchestrator.js -level 2"
  },
  "repository": {
    "type": "git",
    "url": "https://us-south.git.cloud.ibm.com/anac-negotiation/anac-environment-orchestrator"
  },
  "author": "Jeff Kephart",
  "dependencies": {
    "@cel/celio": "^2.14.0",
    "@cel/discover": "^1.1.11",
    "@cel/logger": "^1.0.0",
    "express": "3.4.7",
    "uuid": "*",
    "dotenv": "^4.0.0",
    "lodash": "^4.17.11",
    "prettier": "^1.16.1",
    "xregexp": "^2.0",
    "request-promise": "*"
  },
  "register": {
    "instanceName": "anac-environment-orchestrator",
    "serviceType": "anac-environment-orchestrator",
    "testRoute": "/test",
    "testInterval": 30,
    "launchInvocation": "nohup node anac-environment-orchestrator.js -port 14010 -level 2 > eo001.log &",
    "lcManagerDiscover": {
      "host": "localhost",
      "port": 1111
    },
    "lcManagerRegister": {
      "host": "localhost",
      "port": 1111
    }
  }
}
